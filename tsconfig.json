{
    "compilerOptions": {
      "target": "es2016",
      "module": "commonjs",
      "rootDir": "./src",
      "outDir": "./dist",
      "noEmitOnError": true,
      "esModuleInterop": true,
      "forceConsistentCasingInFileNames": true,
      "strict": true,
      "strictNullChecks": true,
      "skipLibCheck": true
    },
    "include": ["src/**/*.ts"],
    "exclude": ["node_modules"]
  }